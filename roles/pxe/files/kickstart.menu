menu title Kickstart Profiles

label back
  menu label ^Return to Main Menu
  kernel vesamenu.c32
  append pxelinux.cfg/default
label dorothy
  menu label Dorothy Harris
  kernel centos7/vmlinuz
  append initrd=centos7/initrd.img ip=dhcp ks=http://forrest.magicalchicken.com/ks-dorothy.cfg
label tex
  menu label Tex
  kernel centos7/vmlinuz
  append initrd=centos7/initrd.img ip=dhcp net.ifnames=0 ks=http://forrest.magicalchicken.com/ks-tex.cfg
